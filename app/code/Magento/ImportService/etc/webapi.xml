<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
    <route url="/V1/import" method="POST">
        <service class="Magento\ImportService\Api\ImportProcessorInterface" method="executeImport"/>
        <resources>
            <resource ref="Magento_ImportExport::import" />
        </resources>
    </route>
</routes>
